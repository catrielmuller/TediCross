    "mime": "3.0.0",
    "moment": "2.29.3",
    "ramda": "0.27.2",
    "semver": "7.3.5",
    "simple-markdown": "0.7.3",
    "telegraf": "4.11.2",
    "typescript": "4.5.5",
    "yargs": "17.3.1",
    "sqlite3": "5.1.4",
    "sqlite": "4.1.2"
